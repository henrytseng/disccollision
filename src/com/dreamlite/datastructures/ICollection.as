package com.dreamlite.datastructures {	/**	 * @author henry	 */	public interface ICollection {		/**		 * Returns <code>true</code> if collection contains data  		 * @return <code>true</code> if collection contains data  		 */		function contains($data:*):Boolean;				/**		 * Gets an iterator to this collection		 * @return Create a new instance of an <code>IIterator</code>		 */		function get iterator():IIterator;				/**		 * Gets the number of items in collection		 * @return Number of items in collection, otherwise -1 if no items in collection		 */		function get size():int;				/**		 * Returns an array containing all data		 * @return Array containing all data		 */		function toArray():Array;				/**		 * Returns a string representation of data		 */		function toString():String;				/**		 * Destroy		 */		function destroy():void;	}}